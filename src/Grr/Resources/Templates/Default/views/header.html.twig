<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="Robots" content="noindex" />
    <title>{{ title }}</title>
    <link rel="shortcut icon" href="favicon.ico" />
    <link rel="stylesheet" type="text/css" href="src/Grr/Resources/Templates/{{ template }}/public/css/{{ sheetcss }}/style.css" />
    <link rel="stylesheet" type="text/css" href="src/Grr/Resources/Templates/{{ template }}/public/css/{{ sheetcss }}/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="src/Grr/Resources/Templates/{{ template }}/public/css/{{ sheetcss }}/mod_bootstrap.css" />
    {% if use_admin %}
        <link rel="stylesheet" type="text/css" href="include/admin_grr.css" />
    {% endif %}
    <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/jquery-2.1.1.min.js"></script>
    <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/jquery-ui.min.js"></script>
    {% if use_select2 %}
        <link rel="stylesheet" type="text/css" href="src/Grr/Resources/Templates/{{ template }}/public/css/themes/{{ template }}/css/select2.css" />
        <link rel="stylesheet" type="text/css" href="src/Grr/Resources/Templates/{{ template }}/public/css/themes/{{ template }}/css/select2-bootstrap.css" />
        <link rel="stylesheet" type="text/css" href="src/Grr/Resources/Templates/{{ template }}/public/css/themes/{{ template }}/css/bootstrap-multiselect.css">
        <link rel="stylesheet" type="text/css" href="src/Grr/Resources/Templates/{{ template }}/public/css/themes/{{ template }}/css/bootstrap-clockpicker.min.css">

        <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/bootstrap-clockpicker.js"></script>
        <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/bootstrap-multiselect.js"></script>
        <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/select2.js"></script>
        <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/select2_locale_fr.js"></script>
    {% endif %}
    {% if pview %}
        <link rel="stylesheet" type="text/css" href="src/Grr/Resources/Templates/{{ template }}/public/css/themes/print/css/style.css" />
    {% endif %}
    <link rel="stylesheet" type="text/css" href="src/Grr/Resources/Templates/{{ template }}/public/css/themes/{{ template }}/css/jquery-ui.css" />
    <link rel="stylesheet" type="text/css" href="src/Grr/Resources/Templates/{{ template }}/public/css/themes/{{ template }}/css/jquery-ui-timepicker-addon.css" >

    <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/jquery.validate.js"></script>
    <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/jquery-ui-timepicker-addon.js"></script>
    <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/html2canvas.js"></script>
    <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/menu.js"></script>
    <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/jspdf.min.js"></script>
    <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/pdf.js" ></script>
    <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/popup.js" charset="utf-8"></script>
    <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/functions.js" ></script>
    {% if use_tooltip_js %}
        <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/tooltip.js"></script>
    {% endif %}
    {% if selection %}
        <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/selection.js" ></script>
    {% endif %}
    {% if clock_file %}
        <script type="text/javascript" src="src/Grr/Resources/Templates/{{ template }}/public/js/{{ clock_file }}"></script>
    {% endif %}
    {% if notPhp3 != null %}
        {{ notPhp3 }}
    {% endif %}
</head>
<body>

